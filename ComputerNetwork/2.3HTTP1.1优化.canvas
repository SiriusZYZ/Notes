		{"type":"text","text":"## 缓存未过期","id":"96dcf153b31dc968","x":380,"y":-760,"width":220,"height":60},
		{"type":"text","text":"## 缓存过期\n带上Etag发送请求","id":"406fc32c3e76be3b","x":380,"y":-587,"width":220,"height":94},
		{"type":"text","text":"请求URL","id":"b086195a02c5f71f","x":-290,"y":-760,"width":125,"height":60},
